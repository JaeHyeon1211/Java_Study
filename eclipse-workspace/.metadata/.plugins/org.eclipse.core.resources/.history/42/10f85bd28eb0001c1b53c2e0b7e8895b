function register(){
	
	document.memberRegistry.action = '/member/memberRegistry';
	document.memberRegistry.submit();
}


</script>
<body>
<form name = "memberRegistry" method = "get" onsubmit="register()">
	아이디 : <input type = "text" name="userid"><br>
	이름 : <input type = "text" name="username"><br>
	나이 : <input type = "text" name="age"><br>
	<input type="button" value="등록" onclick="register()">
</form>
